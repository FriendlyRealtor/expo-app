{
  "expo": {
    "name": "balance-lifestyle",
    "slug": "balance-lifestyle",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
		"plugins": [
			"expo-tracking-transparency",
			"@react-native-google-signin/google-signin"
		],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
			"bundleIdentifier": "jubileeinvestments.Real-Estate-App",
			"config": {
				"googleSignIn": {
					"reservedClientId": "com.googleusercontent.apps.154068447777-70bbsl9rsmsi27tv3snugb9hj8br2vng"
				}
			},
			"googleServicesFile": "./GoogleService-Info.plist"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "03613ba6-fe49-4a83-9564-6180c192c06a"
      }
    },
    "owner": "trelliski"
  }
}
